 <% if flash[:notice] %>
    <div class="notice"><%= flash[:notice] %></div>
  <% end %>

  <% if flash[:error] %>
    <div class="error"><%= flash[:error] %></div>
  <% end %>

<h1>Account Index page</h1>
<h3> To see the details each account click on it</h3>

<% @accounts.each do |account| %>
<%= link_to account.name, account_path(account), class: "button" %><br><br>
<%end%>
